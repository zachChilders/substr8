{
	"name": "Rust",
	"dockerFile": "Dockerfile",
	"extensions": [
		"rust-lang.rust",
		"bungcip.better-toml",
		"vadimcn.vscode-lldb"
	],
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"settings": {
		"lldb.executable": "/usr/bin/lldb-3.9"
	},
	"appPort": [7878, "7878:7878"]
}